        <label for="first_name_box">
          First name
        </label>

        <input type="text" id="first_name_box" name="query_first_name">
      </div>

      <div>
        <label for="last_name_box">
          Last name
        </label>

        <input type="text" id="last_name_box" name="query_last_name">
      </div>

      <div>
        <label for="mi_box">
          Mi
        </label>

        <input type="text" id="mi_box" name="query_mi">
      </div>

      <div>
        <label for="birth_date_box">
          Birth date
        </label>

        <input type="date" id="birth_date_box" name="query_birth_date">
      </div>

      <div>
        <label for="email_box">
          Email
        </label>

        <input type="text" id="email_box" name="query_email">
      </div>

      <div>
        <label for="phone_box">
          Phone
        </label>

        <input type="text" id="phone_box" name="query_phone">
      </div>

      <div>
        <label for="gender_box">
          Gender
        </label>

        <input type="text" id="gender_box" name="query_gender">
      </div>

      <div>
        <label for="sexuality_box">
          Sexuality
        </label>

        <input type="text" id="sexuality_box" name="query_sexuality">
      </div>

      <div>
        <input type="checkbox" id="is_smoker_box" name="query_is_smoker" value="1">

        <label for="is_smoker_box">Is smoker</label>
      </div>

      <div>
        <input type="checkbox" id="is_pet_owner_box" name="query_is_pet_owner" value="1">

        <label for="is_pet_owner_box">Is pet owner</label>
      </div>

      <div>
        <label for="ethnicity_box">
          Ethnicity
        </label>

